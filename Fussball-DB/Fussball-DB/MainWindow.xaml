<Window x:Class="Fussball_DB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fussball_DB"
        mc:Ignorable="d"
        Title="Bundesliga" Height="1080" Width="1920"
        WindowState="Maximized"
        Icon="bundesliga.png"
        x:Name="win"
        ResizeMode="NoResize">
    <Grid>
        <StackPanel x:Name="grid">
        <Menu>
            <MenuItem Header="_Ausgabe" Click="MenuItem_Ausgabe_Click"/>
            <MenuItem Header="_Ultimate Team" Click="MenuItem_UltimateTeam_Click"/>
            <MenuItem Header="_Tabelle/Spiele" Click="MenuItem_Tabelle_Click"/>
            <MenuItem Header="_About" Click="MenuItem_Click"/>
        </Menu>
        </StackPanel>
    </Grid>
</Window>
